{

	"manifest_version": 2,
	"name": "password-extension",
	"version": "1.1",
	"description": "Simple extension to generate pseudorandom passwords.",

	"icons": {
		"16": "icons/password-16.png",
		"32": "icons/password-32.png",
		"48": "icons/password-48.png",
		"128": "icons/password-128.png"
	},

	"browser_action": {
		"default_icon": {
			"16": "icons/password-16.png",
			"32": "icons/password-32.png",
			"48": "icons/password-48.png",
			"128": "icons/password-128.png"
		},
		"default_title": "Password",
		"default_popup": "popup/password.html"
	}

}
